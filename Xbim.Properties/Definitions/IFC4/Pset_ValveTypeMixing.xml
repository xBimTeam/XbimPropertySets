	<IfcVersion version="2x4" />
	<ApplicableClasses>
		<ClassName>IfcValve/MIXING</ClassName>
	</ApplicableClasses>
	<ApplicableTypeValue>IfcValve/MIXING</ApplicableTypeValue>

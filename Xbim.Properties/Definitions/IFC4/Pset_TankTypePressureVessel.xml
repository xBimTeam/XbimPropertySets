			<PropertyType>
				<TypePropertySingleValue>
					<DataType type="IfcPressureMeasure" />
				</TypePropertySingleValue>
			</PropertyType>
			<NameAliases>
				<NameAlias lang="ja-JP">安全弁設定</NameAlias>
			</NameAliases>
			<DefinitionAliases>
				<DefinitionAlias lang="ja-JP">安全弁が作動する圧力</DefinitionAlias>
			</DefinitionAliases>
		</PropertyDef>
	</PropertyDefs>
	<PsetDefinitionAliases>
		<PsetDefinitionAlias lang="ja-JP">圧力容器の共通属性</PsetDefinitionAlias>
	</PsetDefinitionAliases>

			<PropertyType>
				<TypePropertyEnumeratedValue>
					<EnumList name="PEnum_SwitchingDeviceDimmerSwitchType">
						<EnumItem>ROCKER</EnumItem>
						<EnumItem>SELECTOR</EnumItem>
						<EnumItem>TWIST</EnumItem>
						<EnumItem>OTHER</EnumItem>
						<EnumItem>NOTKNOWN</EnumItem>
						<EnumItem>UNSET</EnumItem>
					</EnumList>
				</TypePropertyEnumeratedValue>
			</PropertyType>

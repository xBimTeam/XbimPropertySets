<?xml version="1.0" encoding="utf-8"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
	<Name>Pset_CoolingTowerPHistory</Name>
	<Definition>Cooling tower performance history attributes.</Definition>
	<IfcVersion version="2x4" />
	<ApplicableClasses>
		<ClassName>IfcCoolingTower</ClassName>
	</ApplicableClasses>
	<ApplicableTypeValue>IfcCoolingTower</ApplicableTypeValue>
	<PropertyDefs>
		<PropertyDef ifdguid="4aba8a00d1cf11e1800000215ad4efdf">
			<Name>Capacity</Name>
			<Definition>Cooling tower capacity in terms of heat transfer rate of the cooling tower between air stream and water stream.</Definition>
			<PropertyType>
				<TypePropertyReferenceValue reftype="IfcTimeSeries">
					<DataType type="IfcPowerMeasure" />
				</TypePropertyReferenceValue>
			</PropertyType>
			<NameAliases />
			<DefinitionAliases />
		</PropertyDef>
		<PropertyDef ifdguid="4db57a80d1cf11e1800000215ad4efdf">
			<Name>HeatTransferCoefficient</Name>
			<Definition>Heat transfer coefficient-area product.</Definition>
			<PropertyType>
				<TypePropertyReferenceValue reftype="IfcTimeSeries">
					<DataType />
				</TypePropertyReferenceValue>
			</PropertyType>
			<NameAliases />
			<DefinitionAliases />
		</PropertyDef>
		<PropertyDef ifdguid="50b06b00d1cf11e1800000215ad4efdf">
			<Name>SumpHeaterPower</Name>
			<Definition>Electrical heat power of sump heater.</Definition>
			<PropertyType>
				<TypePropertyReferenceValue reftype="IfcTimeSeries">
					<DataType type="IfcPowerMeasure" />
				</TypePropertyReferenceValue>
			</PropertyType>
			<NameAliases />
			<DefinitionAliases />
		</PropertyDef>
		<PropertyDef ifdguid="5443f200d1cf11e1800000215ad4efdf">
			<Name>UACurve</Name>
			<Definition>UA value as a  function of fan speed at certain water flow rate, UA = f ( fan speed).</Definition>
			<PropertyType>
				<TypePropertyReferenceValue reftype="IfcTimeSeries">
					<DataType />
				</TypePropertyReferenceValue>
			</PropertyType>
			<NameAliases />
			<DefinitionAliases />
		</PropertyDef>
		<PropertyDef ifdguid="57d77900d1cf11e1800000215ad4efdf">
			<Name>Performance</Name>
			<Definition>Water temperature change as a function of wet-bulb temperature, water entering temperature, water flow rate, air flow rate, Tdiff = f ( Twet-bulb, Twater,in, mwater, mair).</Definition>
			<PropertyType>
				<TypePropertyReferenceValue reftype="IfcTimeSeries">
					<DataType />
				</TypePropertyReferenceValue>
			</PropertyType>
			<NameAliases />
			<DefinitionAliases />
		</PropertyDef>
	</PropertyDefs>
